.base-panel {
  padding: 0 .2rem;
  position: relative;
  z-index: 1;
  background-color: transparent;
  margin-bottom: 0.2rem;
}

.panel-content {
  position: relative;
  padding: 0.25rem;
  background-color: white;
  border-radius: 0.2rem;
  overflow: hidden;
  box-shadow: 0 5px 25px 0 rgba(0, 0, 0, .13);
}